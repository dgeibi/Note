{
  "name": "note",
  "version": "1.2.0",
  "main": "gulpfile.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.1.3"
  },
  "scripts": {
    "start": "gulp",
    "css": "postcss _styles/main.css -o assets/css/main.css",
    "js": "NODE_ENV=production webpack",
    "wikic": "NODE_ENV=production wikic build --clean",
    "build": "npm run js && npm run css && npm run wikic",
    "js:format": "prettier-eslint --write './*.js' './_scripts/**/*.js'",
    "css:format": "stylefmt -r ./_styles/**/*.css",
    "format": "npm run js:format && npm run css:format",
    "lint": "eslint ."
  },
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.6.0",
    "format-json": "^1.0.3",
    "gulp": "^3.9.1",
    "gulp-postcss": "^6.4.0",
    "htmlclean": "^2.7.10",
    "normalize.css": "^6.0.0",
    "postcss": "^6.0.8",
    "postcss-cli": "^4.0.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-csso": "^2.0.0",
    "postcss-import": "^9.1.0",
    "smoothscroll-polyfill": "^0.3.4",
    "uemitter": "0.0.3",
    "unfetch": "^2.1.2",
    "webpack": "^2.3.3",
    "webpack-stream": "^3.2.0",
    "wikic": "^3.0.2",
    "wikic-suite-docslist": "^3.0.0",
    "wikic-suite-docsmap": "^2.0.0"
  },
  "description": "personal wiki",
  "author": "dgeibi <png.inside@gmail.com>",
  "repository": "git@github.com:dgeibi/note.git",
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-config-dgeibi": "dgeibi/eslint-config-dgeibi",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-no-async-without-await": "^1.2.0",
    "prettier-eslint-cli": "^4.0.0",
    "stylefmt": "^5.3.2"
  }
}
